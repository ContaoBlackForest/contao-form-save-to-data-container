{
    "name": "contaoblackforest/contao-form-save-to-data-container",
    "description": "Save form data to data container for Contao OpenSource CMS",
    "keywords": [
        "contao",
        "frontend",
        "form",
        "save",
        "news",
        "bundle"
    ],
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Sven Baumann",
